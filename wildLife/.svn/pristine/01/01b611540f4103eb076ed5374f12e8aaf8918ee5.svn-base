                                    repeat="exportReason.name as exportReason in vm.exportReason | propsFilter: {name: $select.search}">
                                    <div class="bold" ng-bind-html="exportReason.name | highlight: $select.search"></div>
                                </ui-select-choices>
                            </ui-select>
                        </td>
                        <td>
                            <input style="width: 56.8px;" data-ng-model="reportForm.note" type="text" ng-class="{'glb-disabled':reportForm.editable == false}" class="form-control"/>
                        </td>
                        <!-- <td class="text-center" style="max-width: 100px;min-width: 100px;">
                            <a ng-click="vm.deleteForm16Record($index,reportForm.id)"><i class="text-danger">Xóa</i></a>
                        </td> -->
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

</div>
