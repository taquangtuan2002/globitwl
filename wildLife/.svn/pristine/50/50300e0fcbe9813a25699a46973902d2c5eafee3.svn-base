<div class="modal-header">
	<button type="button" class="close" ng-click="$dismiss()" aria-hidden="true"></button>
	<h4 ng-if="vm.exportAnimalFeed.isNew == true" class="modal-title"><i
			class="icon-plus"></i>{{'animal.create' | translate}}</h4>
	<h4 ng-if="vm.exportAnimalFeed.isNew == false" class="modal-title"><i
			class="icon-pencil"></i>{{'animal.edit' | translate}}</h4>
</div>
<div class="modal-body">

	<div class="row">
		<div class="col-md-12">
			<h3 style="color:green; text-align:center">{{"importAnimalFeed.farm" | translate}}
				{{vm.exportAnimalFeed.farm.name}}</h3>
			<!--<input readonly	data-ng-model="vm.exportAnimalFeed.farm.name" type="text" class="form-control" title="vm.code"/>-->
		</div>
		<div class="col-md-12 margin-top-10" ng-if="!vm.isFamer">
			<label class="bold"><strong style="color:red">*</strong>{{"importAnimal.CSCN" | translate}}</label>
			<div class="input-group">
				<input readonly data-ng-model="vm.exportAnimalFeed.farm.name"
					placeholder="{{'exportAnimal.searchFarm' | translate}}" type="text" class="form-control"
					title="vm.code" />
				<div class="input-group-btn">
					<button ng-click="vm.showPopupFarm()" class="btn btn-default" type="submit"><i
							class="glyphicon glyphicon-plus"></i>{{"exportAnimal.choose" | translate}}</button>
				</div>
			</div>
		</div>

		<div class="col-md-12 margin-top-10">
			<label class="bold"><strong style="color:red">*</strong>{{"genarel.batchCode" | translate}}</label>
			<div class="input-group">
				<input readonly data-ng-model="vm.exportAnimalFeed.importAnimalFeed.code"
					placeholder="{{'genarel.searchBatchCode' | translate}}" type="text" class="form-control"
					title="vm.code" />
				<div class="input-group-btn">
					<button ng-click="vm.showPopupBatchCode()" class="btn btn-default" type="submit"><i
							class="glyphicon glyphicon-plus"></i>{{"exportAnimal.choose" | translate}}</button>
				</div>
			</div>
		</div>

		<div class="col-md-12 margin-top-10">
			<label class="bold"><strong style="color:red">*</strong>{{"importAnimalFeed.bran" | translate}}:</label>
			<input data-ng-model="vm.exportAnimalFeed.importAnimalFeed.bran.name" disabled type="text" class="form-control" />
		</div>

		<div class="col-md-12 margin-top-10">
			<label class="bold"><strong style="color:red">*</strong>{{"application.menu.sku" | translate}}:</label>
			<input data-ng-model="vm.exportAnimalFeed.importAnimalFeed.unit.name" disabled type="text" class="form-control" />
		</div>
		<div class="col-md-12 margin-top-10">
			<label class="bold">{{"importAnimalFeed.price"|translate}}:</label>
			<input type="text" number-input ng-model="vm.exportAnimalFeed.importAnimalFeed.price" disabled name="price" class="form-control" />
		</div>
		<div class="col-md-12 margin-top-10">
			<label class="bold"><strong style="color:red">*</strong>{{"importAnimalFeed.remain_quantity"|translate}}:</label>
			<input type="text" number-input ng-model="vm.exportAnimalFeed.importAnimalFeed.remainQuantity" disabled name="remain_quantity"
				class="form-control" />
		</div>

		<div class="col-md-12 margin-top-10">
			<label class="bold"><strong style="color:red">*</strong>{{"genarel.dateExport" | translate}}:</label>
			<input data-ng-model="vm.exportAnimalFeed.dateIssue" type="text" my-date-picker class="form-control" />
		</div>

		<div class="col-md-12 margin-top-10">
			<label class="bold"><strong style="color:red">*</strong>{{"importAnimalFeed.export_quantity"|translate}}:</label>
			<input type="text" number-input ng-model="vm.exportAnimalFeed.quantity" name="quantity"
				class="form-control" />
		</div>
		<div class="col-md-12 margin-top-10">
			<label class="bold">{{"importAnimalFeed.buyerName"|translate}}:</label>
			<input data-ng-model="vm.exportAnimalFeed.buyerName" type="text" class="form-control" />
		</div>
		<div class="col-md-12 margin-top-10">
			<label class="bold">{{"importAnimalFeed.buyerAdress"|translate}}:</label>
			<input data-ng-model="vm.exportAnimalFeed.buyerAdress" type="text" class="form-control" />
		</div>
		<div class="col-md-12 margin-top-10">
			<label class="bold">{{"importAnimal.description"|translate}}:</label>
			<input data-ng-model="vm.exportAnimalFeed.description" type="text" class="form-control" />
		</div>
	</div>

</div>
<div class="modal-footer">
	<button type="button" ng-click="$dismiss()" class="btn btn-default" data-dismiss="modal"><i class="icon-close"></i>
		{{'animal.cancel' | translate}}</button>
	<button type="button" ng-click="vm.save(vm.exportAnimalFeed)" class="btn btn-primary"
		data-dismiss="modal">{{'animal.save' | translate}}</button>

</div>
<script type="text/ng-template" id="bran_detail.html">
	<div modal-movable>
		<div ng-include="'bran/views/bran_info.html'"></div>
	</div>
</script>
<script type="text/ng-template" id="choose_farm_modal.html">
	<div modal-movable>
		<div class="modal-header">
			<button type="button" class="close" ng-click="$dismiss()" aria-hidden="true"></button>
			<h4 class="modal-title bold font-yellow-gold"><i class="icon-question"></i> {{"exportAnimal.chooseFarm" | translate}}</h4>
		</div>
		<div class="modal-body">
			<div class="box-body box-profile">
				<div class="portlet-title">

					<div class="row margin-top-10 margin-bottom-10">
						<div class="col-md-6">
						</div>

						<div class="col-md-6">
							<div class="input-group">
								<input type="text"  ng-if="vm.isFamer == false"  data-ng-model="vm.farmCode" class="form-control" ng-keyup="vm.enterSearchFarm(e)" placeholder="{{'farm.searchNameCode' | translate}}" name="search">
								<input type="text"  ng-if="vm.isFamer == true" readonly data-ng-model="vm.farmCode" class="form-control" ng-keyup="vm.enterSearchFarm(e)" placeholder="{{'farm.searchNameCode' | translate}}" name="search">
								<div class="input-group-btn">
									<button ng-click="vm.searchByFarm()" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
								</div>
							</div>
						</div>
					</div>
					<!--<button class="btn btn-danger margin-left-10" data-ng-click="vm.deleteLabs()" data-ng-disabled="vm.selectedSpecialties.length <= 0"><i class="fa fa-trash"></i> XÃ³a báº£n ghi Ä‘Ã£ chá»�n</button>-->
					<!--</div>-->
				</div>
				<div class="box-body">
					<table id="bsTableControlFarm" bs-table-control="vm.bsTableControlFarm"></table>

				</div>
			</div>
		</div>
		<div class="modal-footer">
			<button type="button" ng-click="$dismiss()" class="btn btn-default" data-dismiss="modal">{{"exportAnimal.close" | translate}}</button>
			<button type="button" ng-click="vm.agreeFarm()" class="btn btn-primary" data-dismiss="modal">{{"exportAnimal.yes" | translate}}</button>
		</div>
	</div>
</script>


<script type="text/ng-template" id="choose_batch_code_modal.html">
	<div modal-movable>
		<div class="modal-header">
			<button type="button" class="close" ng-click="$dismiss()" aria-hidden="true"></button>
			<h4 class="modal-title bold font-yellow-gold"><i class="icon-question"></i>{{"exportAnimal.chooseBatchCode" | translate}}</h4>
		</div>
		<div class="modal-body">
			<div class="box-body box-profile">
				<div class="portlet-title">

					<div class="row margin-top-10 margin-bottom-10">
						<div class="col-md-6">
						</div>

						<div class="col-md-6">
							<div class="input-group">
								<input type="text" data-ng-model="vm.batchCode" class="form-control" ng-keyup="vm.enterSearchBatchCode(e)" placeholder="{{'exportAnimal.searchNameCode' | translate}}" name="search">
								<div class="input-group-btn">
									<button ng-click="vm.searchByBatchCode()" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
								</div>
							</div>
						</div>
					</div>
					<!--<button class="btn btn-danger margin-left-10" data-ng-click="vm.deleteLabs()" data-ng-disabled="vm.selectedSpecialties.length <= 0"><i class="fa fa-trash"></i> Xóa bản ghi đã chọn</button>-->
					<!--</div>-->
				</div>
				<div class="box-body">
					<table id="bsTableControlCode" bs-table-control="vm.bsTableControlCode"></table>

				</div>
			</div>
		</div>
		<div class="modal-footer">
			<button type="button" ng-click="$dismiss()" class="btn btn-default" data-dismiss="modal">{{"exportAnimal.close" | translate}}</button>
			<button type="button" ng-click="vm.agree()" class="btn btn-primary" data-dismiss="modal">{{"exportAnimal.yes" | translate}}</button>
		</div>
	</div>
</script>