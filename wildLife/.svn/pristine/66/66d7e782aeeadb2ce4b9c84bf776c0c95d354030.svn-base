                                    <div class="input-group">
                                        <input ng-model="vm.nameOrCodeForestProductList" type="text"
                                            ng-keypress="vm.enterSearchForestProductList($event)"
                                            placeholder='Tài khoản người tạo, tên người tạo, mã kê lâm sản'
                                            class="form form-control" />
                                        <div class="input-group-btn">
                                            <button ng-click="vm.searchByCodeForestProductList()"
                                                class="btn btn-default" type="submit"><i
                                                    class="glyphicon glyphicon-search"></i></button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="portlet-body">
                    <table id="bsTableControlForestProductList" bs-table-control="vm.bsTableControlForestProductList">
                    </table>
                </div>
            </div>
