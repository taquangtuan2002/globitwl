        <button ng-if="vm.isFamer == false && vm.isSdahView==false" type="button"
            class="btn btn-default btn-primary margin-top-10" ng-click="vm.saveFarm()">
            {{'farm.save' | translate}}
        </button>
    </div>
</div>

