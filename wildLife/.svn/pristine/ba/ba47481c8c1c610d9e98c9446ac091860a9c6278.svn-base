	public AnimalRaisingReportDto(ImportExportAnimal iea) {
		super();
		if(iea != null) {
			Animal animal = iea.getAnimal();
			if(animal != null) {
				this.animalCode = animal.getCode();
				this.animalName = animal.getName();
				this.animalScienceName = animal.getScienceName();
				if(iea.getAnimal().getOriginal() != null) {
					this.originName = iea.getAnimal().getOriginal().getName();
					this.originCode = iea.getAnimal().getOriginal().getCode();
				}
			}
			if(iea.getSeedLevel() != null) {
				seedLevelId = iea.getSeedLevel().getId();
				this.level = iea.getSeedLevel().getLevel();
			}
			this.batchCode = iea.getBatchCode();
			if(iea.getProductTarget() != null) {
				this.productTargetName = iea.getProductTarget().getName();
				this.productTargetCode = iea.getProductTarget().getCode();
			}
			this.type = iea.getType();
			this.quantity = iea.getQuantity();
			this.gender = iea.getGender();
		}
		
	}
	
	public AnimalRaisingReportDto(String animalCode, Double total) {
