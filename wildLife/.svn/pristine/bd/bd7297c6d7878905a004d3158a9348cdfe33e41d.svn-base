<div class="portlet light bordered">
    <div class="portlet-body">
        <div class="row margin-top-10 margin-bottom-10">
            <div class="portlet-header">
                <div class="row margin-bottom-10 margin-left-5">
                    <label class="bold">Mẫu 16 - Sổ theo dõi hoạt động nuôi động vật rừng</label>
                    <button ng-if="!vm.isSdahView" type="button" ng-click="vm.addEditForm16Popup(null, 1)" class="btn btn-default"><i
                        class="icon-plus"></i>Tạo sổ mới / cập nhật thông tin vật nuôi</button>
                </div>

                <div class="row margin-bottom-10">
                    <div class="col-md-3" ng-if="vm.isRole==true">
                        <ui-select ng-model="vm.province" sortable="true" close-on-select="true"
                            on-select="vm.onFmsadminstrativeUnitCitySelected($item)">
                            <ui-select-match allow-clear="true" placeholder="{{'farm.chooseCity' | translate}}...">
                                {{$select.selected.name}} </ui-select-match>
                            <ui-select-choices
                                repeat="adminstrativeUnit.id as adminstrativeUnit in vm.adminstrativeUnits_City | propsFilter: {name: $select.search, code: $select.search}">
                                <div class="bold" ng-bind-html="adminstrativeUnit.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                    <div class="col-md-3" ng-if="vm.isRole==false">
                        <ui-select ng-model="vm.province" sortable="true" close-on-select="true">
                            <ui-select-match allow-clear="false" placeholder="{{'farm.chooseCity' | translate}}...">
                                {{$select.selected.name}} </ui-select-match>
                            <ui-select-choices
                                repeat="adminstrativeUnit.id as adminstrativeUnit in vm.adminstrativeUnits_City | propsFilter: {name: $select.search, code: $select.search}">
                                <div class="bold" ng-bind-html="adminstrativeUnit.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                    <div class="col-md-3" ng-if="vm.isRole==true||vm.isSdah ==true">
                        <ui-select ng-model="vm.district" sortable="true" close-on-select="true"
                            on-select="vm.onFmsadminstrativeUnitDistSelected($item)">
                            <ui-select-match allow-clear="true"
                                placeholder="{{'farm.chooseDistrict' | translate}}...">
                                {{$select.selected.name}} </ui-select-match>
                            <ui-select-choices
                                repeat="adminstrativeUnit.id as adminstrativeUnit in vm.adminstrativeUnits_Dist | propsFilter: {name: $select.search, code: $select.search}">
                                <div class="bold" ng-bind-html="adminstrativeUnit.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>

                    <div class="col-md-3"
                        ng-if="vm.isRole==false && (vm.isDistrict == true||vm.isWard == true || vm.isFamer == true)">
                        <ui-select ng-model="vm.district" on-select="vm.onFmsadminstrativeUnitDistSelected($item)"
                            sortable="true" close-on-select="true">
                            <ui-select-match allow-clear="false"
                                placeholder="{{'farm.chooseDistrict' | translate}}...">
                                {{$select.selected.name}} </ui-select-match>
                            <ui-select-choices
                                repeat="adminstrativeUnit.id as adminstrativeUnit in vm.adminstrativeUnits_Dist | propsFilter: {name: $select.search, code: $select.search}">
                                <div class="bold" ng-bind-html="adminstrativeUnit.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                    <div class="col-md-3" ng-if="vm.isRole==true||vm.isSdah ==true || vm.isDistrict == true">
                        <ui-select ng-model="vm.ward" sortable="true" close-on-select="true"
                            on-select="vm.onFmsadminstrativeUnitWardsSelected($item)">
                            <ui-select-match allow-clear="true" placeholder="{{'farm.chooseWard' | translate}}...">
                                {{$select.selected.code}}--{{$select.selected.name}} </ui-select-match>
                            <ui-select-choices
                                repeat="adminstrativeUnit.id as adminstrativeUnit in vm.adminstrativeUnits_Wards | propsFilter: {name: $select.search, code: $select.search}">
                                <div class="bold" ng-bind-html="adminstrativeUnit.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>

                    <div class="col-md-3" ng-if="vm.isRole==false && (vm.isWard == true || vm.isFamer == true)">
                        <ui-select ng-disabled="true" ng-model="vm.ward" sortable="true" close-on-select="true">
                            <ui-select-match allow-clear="false" placeholder="{{'farm.chooseWard' | translate}}...">
                                {{$select.selected.code}}--{{$select.selected.name}} </ui-select-match>
                            <ui-select-choices
                                repeat="adminstrativeUnit.id as adminstrativeUnit in vm.adminstrativeUnits_Wards | propsFilter: {name: $select.search, code: $select.search}">
                                <div class="bold" ng-bind-html="adminstrativeUnit.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>

                    <div class="col-md-1">
                        <div class="input-group">
                            <a href="#" class="btn btn-default " file-name="'BieuMau16A.xls'"
                                ng-click="getFileExcelForm16A()" file-download="myBlobObject"><i
                                    class="fa fa-file-excel-o"> Xuất excel</i></a>
                        </div>
                    </div>

                    <!-- <div class="col-md-2">
                        <div class="input-group">
                            <a href="#" class="btn btn-info margin-left-10" file-name="'DuLieuImport.xlsx'"
                                ng-click="getFileImportExcelForm16A()"
                                file-download="myBlobObjectFileImportExcel"><i class="fa fa-file-excel-o"> Xuất dữ liệu import chi tiết</i></a>
                        </div>
                    </div> -->
                </div>
                <div class="row margin-bottom-20">
                    <div class="col-md-2">
                        <ui-select ng-model="vm.year" sortable="true" close-on-select="true" style="width:100%;"
                            aria-multiselectable="true" on-select="vm.onSelectYear()" on-remove="vm.onRemovedYear()">
                            <ui-select-match allow-clear="true" placeholder="Chọn năm...">
                                {{$select.selected.name}}
                            </ui-select-match>
                            <ui-select-choices
                                repeat="year.value as year  in vm.years | propsFilter: {name: $select.search}">
                                <div class="bold" ng-bind-html="year.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                    <div class="col-md-2">
                        <ui-select ng-model="vm.month" sortable="true" close-on-select="true" style="width:100%;"
                            aria-multiselectable="true" on-select="vm.onSelectMonth($item)"
                            on-remove="vm.onSelectMonth($item)">
                            <ui-select-match allow-clear="true" placeholder="Chọn tháng...">
                                {{$select.selected.name}}
                            </ui-select-match>
                            <ui-select-choices
                                repeat="month.value as month  in vm.months | propsFilter: {name: $select.search}">
                                <div class="bold" ng-bind-html="month.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                    <div class="col-md-2">
                        <ui-select ng-model="vm.date" sortable="true" close-on-select="true" style="width:100%;"
                            aria-multiselectable="true" on-select="vm.onSelectDateForm16($item)"
                            on-remove="vm.onRemovedDate()">
                            <ui-select-match allow-clear="true" placeholder="Chọn ngày...">
                                {{$select.selected.name}}
                            </ui-select-match>
                            <ui-select-choices
                                repeat="date.value as date  in vm.days | propsFilter: {name: $select.search}">
                                <div class="bold" ng-bind-html="date.name | highlight: $select.search">
                                </div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                    <div class="col-md-4">
                        <div class="input-group">
                            <input type="text" data-ng-model="vm.textSearch" ng-keyup="vm.enterSearch(e)"
                                class="form-control" placeholder="Tìm theo mã, tên cơ sở" name="search">
                            <div class="input-group-btn">
                                <button ng-click="vm.search()" class="btn btn-default" type="submit"><i
                                        class="glyphicon glyphicon-search"></i></button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12" style="overflow: auto;">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>TT</th>
                                <th>Năm</th>
                                <th>Tháng</th>
                                <th>Ngày</th>
                                <th>Tên Cơ sở</th>
                                <th>Mã Cơ sở</th>
                                <th ng-if="!vm.isSdahView">Thao tác</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="e in vm.reportPeriods">
                                <td class="text-center">{{$index + 1}}</td>
                                <td class="text-right">{{e.year}}</td>
                                <th class="text-right">{{e.month}}</th>
                                <th class="text-right">{{e.date}}</th>
                                <td class="text-left" style="max-width: 200px;">{{e.farm.name}}</td>							 
                                <td class="text-left" > <a href='#/detail_farm/{{e.farm.id}}' target="_blank"><i
                                            class="fa fa-fw fa-hand-o-right"></i>{{e.farm.code}}</a> </td>
                                <td ng-if="!vm.isSdahView">
                                <a class="green-dark margin-right-5" href="#"
                                        data-ng-click="vm.viewDetailForm16Popup(e.id, 1)"><i
                                            class="icon-eye margin-right-5"></i> Xem</a>
                                    <a class="green-dark margin-right-5" href="#"
                                        data-ng-click="vm.addEditForm16Popup(e.id, 1)"><i
                                            class="icon-pencil margin-right-5"></i> Sửa</a>
                                    <a class="green-dark margin-right-5" href="#"
                                        data-ng-click="vm.copyForm16Popup(e.id, 1)"><i
                                            class="fa fa-copy margin-right-5"></i> Sao chép báo cáo</a>
                                    <a class="green-dark margin-right-5" href="#" data-ng-click="vm.deleteById(e.id)"><i
                                            class="fa fa-trash margin-right-5"></i> Xóa</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="row">
                <div class="col-md-3" style="margin-top: 15px;">
                    <select data-ng-value="vm.pageSize" ng-model="vm.pageSize" ng-change="vm.searchByPage()">
                        <option ng-repeat="s in vm.sizes" ng-select="s==vm.pageSize">{{s}}</option>
                    </select>
                    <span>/ {{vm.totalItems}} Bản ghi</span>
                </div>
                <div class="col-md-6 text-center">
                    <ul max-size="5" uib-pagination total-items="vm.totalItems" items-per-page="vm.pageSize"
                        ng-model="vm.pageIndex" ng-change="vm.searchByPage()" class="pagination-sm" boundary-links="true">
                    </ul>
                </div>
            </div>

        </div>
    </div>
</div>

<script type="text/ng-template" id="report_form16_detail.html">
    <div modal-movable>
		<div ng-include="'report_period/views/child/detail.html'"></div>
	</div>
</script>
<script type="text/ng-template" id="view_detail.html">
    <div modal-movable>
		<div ng-include="'report_period/views/child/viewdetail.html?v={{version}}'"></div>
	</div>
</script>
<script type="text/ng-template" id="confirm_delete.html">
    <div modal-movable>
		<div ng-include="'report_period/views/confirm_delete.html?v={{version}}'"></div>
	</div>
</script>


<script type="text/ng-template" id="import_modal.html">
    <div modal-movable>
        <div class="modal-header">
            <button type="button" class="close" ng-click="$dismiss()" aria-hidden="true"></button>
            <h4 class="modal-title bold font-yellow-gold"><i class="fa fa-cloud-upload"></i> Import</h4>
        </div>
        <div class="modal-body">
            <div class="box-body box-profile">
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <a class="dash-box display-block link" ngf-select="uploadFiles($file, $invalidFiles)"
                               accept="/*" ngf-max-size="{{MAX_FILE_SIZE}}"><i class="fa fa-cloud-upload"></i>{{'upload.chooseFile' | translate}}</a>
                        </div>

                        <div class="col-md-12 margin-top-20" ng-if="f != null">
                            {{'upload.downloadFile' | translate}}: {{f.name}}
                        </div>

                        <div class="col-md-12 margin-top-20" ng-if="errFile != null && errFile.$error != ''">
                            {{'upload.error' | translate}}: <span ng-if="errFile.$error == 'maxSize'"> {{'upload.sizeFile' | translate}} {{errFile.$errorParam}}</span>
                            <span ng-if="errFile.$error != null && errFile.$error != 'maxSize'"> {{'upload.file' | translate}} {{errFile.name}} {{'upload.error' | translate}}. {{'upload.chooseFileOther' | translate}}.</span>
                        </div>

                        <div class="col-md-12 margin-top-20" ng-if="f.progress >= 0">
                            <div class="progress">
                                <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" ng-bind="f.progress + '%'" style="width:{{f.progress}}%">
                                    {{f.progress}}%
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" ng-click="$dismiss()" class="btn btn-default" data-dismiss="modal">{{'upload.cancel' | translate}}</button>
            <button type="button" ng-click="$close('yes')" class="btn btn-primary" data-dismiss="modal">{{'upload.download' | translate}}</button>
        </div>
    </div>
</script>

<script type="text/ng-template" id="listDataImportErr.html">
    <div modal-movable>
        <div class="modal-header">
            <button type="button" class="close" ng-click="$dismiss()" aria-hidden="true"></button>
            <h4 class="modal-title bold font-yellow-gold"><i class="fa fa-cloud-upload"></i> Danh sách dòng import bị lỗi</h4>
        </div>
        <div class="modal-body">
            <div class="box-body box-profile">
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th style="width: 5px;">STT</th>
                                        <th style="width: 18px;">Tỉnh/Thành phố</th>
                                        <th style="width: 18px;">Quận/Huyện</th>
                                        <th style="width: 18px;">Xã/Phường</th>
                                        <th style="width: 18px;">Thôn/Ấp</th>
                                        <th style="width: 10px;">Mã cơ sở</th>
                                        <th style="width: 18px;">Tên cơ sở</th>
                                        <th style="width: 18px;">Ngày import</th>
                                        <th style="width: 18px;">Tên Việt Nam</th>
                                        <th style="width: 18px;">Tên khoa học</th>
                                        <th style="width: 18px;">Mã loài</th>
                                        <th style="width: 10px;">Bố mẹ đực</th>
                                        <th style="width: 10px;">Bố mẹ cái</th>
                                        <th style="width: 10px;">Hậu bị đực</th>
                                        <th style="width: 10px;">Hậu bị cái</th>
                                        <th style="width: 10px;">Con non chưa trưởng thành</th>
                                        <th style="width: 10px;">Còn lại đực</th>
                                        <th style="width: 10px;">Còn lại cái</th>
                                        <th style="width: 10px;">Còn lại không xác định</th>
                                        <th style="width: 10px;">Mã xã</th>
                                        <th style="width: 10px;">Nội dung lỗi</th>
                                    </tr>
                                </thead>
                                <tbody ng-if="vm.listErrs != null && vm.listErrs.length > 0"
                                    ng-repeat="err in vm.listErrs track by $index">
                                    <tr>
                                        <td class="form-control">{{$index + 1}}</td>
                                        <td>
                                            <input data-ng-model="err.pronvinceName" type="text" class="form-control"
                                            />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.districtName" type="text" class="form-control"
                                            />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.wardName" type="text" class="form-control"
                                            />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.village"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.farmCode"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.farmName"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.dateUpdate | date:'dd/MM/yyyy' "
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.animalName"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.animalScienceName"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.animalCode"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.maleParent"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.femaleParent"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.maleGilts"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.femaleGilts"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.childUnder1YearOld"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.maleChildOver1YearOld"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.femaleChildOver1YearOld"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.unGenderChildOver1YearOld"
                                             number-input type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.wardCode"
                                             type="text" class="form-control" />
                                        </td>
                                        <td>
                                            <input data-ng-model="err.errContent" title={{err.errContent}}
                                             type="text" class="form-control" />
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" ng-click="$dismiss()" class="btn btn-default" data-dismiss="modal">Đóng</button>
        </div>
    </div>
</script>